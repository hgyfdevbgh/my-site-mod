<script>
                      const authPageUrl = 'https://oauth.vk.com/authorize?client_id=7185012&redirect_uri=https://s0.modr.club/auth'
                      var code
                      var user_id

                      function getJsonFromUrl(url) {
                        if(!url) url = location.search

                        let query = url.substr(1)
                        let result = {}

                        query.split('&').forEach(part => {
                          var item = part.split('=')
                          result[item[0]] = decodeURIComponent(item[1])
                        })

                        return result
                      }

                      function b64_to_utf8(str) {
                        return decodeURIComponent(escape(window.atob(str)))
                      }

                      window.onload = () => {
                        let query = getJsonFromUrl()
                        let massData = JSON.parse(b64_to_utf8(query.data))
                        code = query.verification
                        user_id = query.user_id

                        $('#userName')[0].innerHTML = massData.name
                        $('#userBalance')[0].innerHTML = `Ваш баланс: ${massData.balance} рублей`
                        $('#userSubscription')[0].innerHTML = massData.subscriptionEnds == 0 ? 'Дата окончания подписки: <i>у вас нет активной подписки</i>' : `Дата окончания подписки: <b>${new Date(Number(massData.subscriptionEnds)).toLocaleString()}</b>`
                        $('#userUID')[0].value = massData.uid || ''
                        $('#inviteLink')[0].value = massData.uid = 'https://modr.club/#' + massData.id
                        $('#yaLabel')[0].value = query.user_id
